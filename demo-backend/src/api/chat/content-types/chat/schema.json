{
  "kind": "collectionType",
  "collectionName": "chats",
  "info": {
    "singularName": "chat",
    "pluralName": "chats",
    "displayName": "Chat"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "game",
        "group",
        "personal"
      ],
      "required": false,
      "default": "personal"
    },
    "name": {
      "type": "string"
    },
    "chat_members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat-member.chat-member",
      "mappedBy": "chat"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "chat"
    }
  }
}
